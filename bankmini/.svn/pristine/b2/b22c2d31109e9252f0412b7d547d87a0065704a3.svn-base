package com.huayu.irla.core.holiday.vo;

import com.huayu.irla.core.base.BaseVO;
/**
 * 节日实体类
 * @author ggt
 * @date 2018-07-24
 */
public class HolidayVO extends BaseVO{
	private static final long serialVersionUID = -2360530180833880324L;
	/**
	 * 表情id
	 */
	private Integer Id;
	/**
	 * 节日编码
	 */
	private String holidayCode;
	/**
	 * 节日名称
	 */
	private String holidayName;
	/**
	 * 节日图片
	 */
	private String holidayImg;
	public Integer getId() {
		return Id;
	}
	public void setId(Integer id) {
		Id = id;
	}
	public String getHolidayCode() {
		return holidayCode;
	}
	public void setHolidayCode(String holidayCode) {
		this.holidayCode = holidayCode;
	}
	public String getHolidayName() {
		return holidayName;
	}
	public void setHolidayName(String holidayName) {
		this.holidayName = holidayName;
	}
	public String getHolidayImg() {
		return holidayImg;
	}
	public void setHolidayImg(String holidayImg) {
		this.holidayImg = holidayImg;
	}
	
	
}
