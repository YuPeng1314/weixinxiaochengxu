package com.huayu.irla.core.comment.vo;

import com.huayu.irla.core.base.BaseVO;

public class CommentVO extends BaseVO{
	private static final long serialVersionUID = 6935147356857020816L;
	/**
	 * 评论id
	 */
	private Integer id;
	/**
	 * 评论内容
	 */
	private String comment;
	/**
	 * 评论资源编码
	 */
	private String resourseCode;
	/**
	 * 评论用户编码
	 */
	private String userCode;
	/**
	 * 用户头像
	 */
	private String photo;
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getComment() {
		return comment;
	}
	public void setComment(String comment) {
		this.comment = comment;
	}
	public String getResourseCode() {
		return resourseCode;
	}
	public void setResourseCode(String resourseCode) {
		this.resourseCode = resourseCode;
	}
	public String getUserCode() {
		return userCode;
	}
	public void setUserCode(String userCode) {
		this.userCode = userCode;
	}
	public String getPhoto() {
		return photo;
	}
	public void setPhoto(String photo) {
		this.photo = photo;
	}
	
	
}
